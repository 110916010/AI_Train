    "import cv2\n",
    "\n",
    "img = cv2.imread('myBaby.jpg') #把讀出來的檔案放在img的陣列中\n",
    "print(img.shape[0])\n",
    "print(img.shape[1])\n",
    "\n",
    "img2 = cv2.resize(img,(512,256),interpolation=cv2.INTER_LINEAR)\n",
    "cv2.imshow('source',img)\n",
    "cv2.imshow('dest',img2)\n",
    "cv2.waitKey(0)"
